{
  "name": "github-emojis",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@9.4.0",
  "description": "All GitHub's emojis in one place.",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/github-emojis",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/github-emojis"
  },
  "bugs": {
    "url": "https://github.com/luxass/github-emojis/issues"
  },
  "keywords": [
    "github",
    "github emojis",
    "emoji",
    "emojis"
  ],
  "workspaces": [
    "www"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./emojis": "./emojis.json",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "emojis.json"
  ],
  "scripts": {
    "build": "unbuild",
    "update:list": "bun run ./scripts/update-list.ts",
    "dev": "vitest",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "www:build": "nuxt build www",
    "www:dev": "nuxt dev www",
    "www:generate": "nuxt generate www",
    "www:preview": "nuxt preview www"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.3.2",
    "@types/node": "^18.16.19",
    "bun-types": "^1.0.2",
    "eslint": "^8.48.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.4"
  }
}
